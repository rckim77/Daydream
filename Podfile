# platform :ios, '9.0'

target 'Daydream' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Daydream
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  pod 'MBProgressHUD', '~> 1.1.0'
  pod 'SwiftLint'
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON'
end
